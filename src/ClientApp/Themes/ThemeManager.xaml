<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <!-- Theme Manager: maps logical theme names to color dictionaries -->
  <ResourceDictionary x:Key="Theme.Light">
    <ResourceDictionary.MergedDictionaries>
      <ResourceDictionary Source="Colors.xaml" />
    </ResourceDictionary.MergedDictionaries>
  </ResourceDictionary>
  <ResourceDictionary x:Key="Theme.Dark">
    <ResourceDictionary.MergedDictionaries>
      <ResourceDictionary Source="Colors.Dark.xaml" />
    </ResourceDictionary.MergedDictionaries>
  </ResourceDictionary>
  <ResourceDictionary x:Key="Theme.HighContrast">
    <ResourceDictionary.MergedDictionaries>
      <ResourceDictionary Source="Colors.HighContrast.xaml" />
    </ResourceDictionary.MergedDictionaries>
  </ResourceDictionary>
</ResourceDictionary>